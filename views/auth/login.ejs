 <section class="content-wrapper">
        <div class="row">
            <div class="eleven column">
                <p class="lead">Login</p>
                <div id="contact-form-area">
                    <% if(flash && flash.err) { %>
                      <ul class="alert alert-success" type="none">
                        <% Object.keys(flash.err).forEach(function(error) { %>
                          <li><%= JSON.stringify(flash.err[error]) %></li>
                        <% }) %>
                        </ul>
                    <% } %>
                    <!-- Contact Form Start //-->
                    <form action="/user/login" id="contactform"> 
                        <fieldset> 
                        <label>Username</label>                           
                        <input type="text" name="email" class="textfield" id="username" value="" /> 
                        <label>Password</label>                           
                        <input type="password" name="password" class="textfield" id="password" value="" />
                        <div class="clear"></div> 
                        <label>&nbsp;</label>
                        <button type="submit" class="buttoncontact" id="buttonsend">Login</button>
                        <span class="loading" style="display: none;">Please wait..</span>
                        <div class="clear"></div>
                        </fieldset> 
                    </form>
                    <!-- Contact Form End //-->
                </div>  
            </div>     
        </div>              
    </section>