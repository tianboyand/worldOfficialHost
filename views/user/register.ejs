<section class="content-wrapper">
        <div class="row">
            <div class="twelve column">
                <p class="lead">Add Member</p>
                <div id="contact-form-area">
                    <!-- Contact Form Start //-->
                    <form action="/user/create" id="contactform"> 
                        <fieldset> 
                        <label>Username <em>*</em></label>                           
                        <input type="text" name="username" class="textfield" id="username" value="" required/> 
                        <label>Nama <em>*</em></label>                           
                        <input type="text" name="nama" class="textfield" id="nama" value="" required/>
                        <label>No. HP <em>*</em></label>                           
                        <input type="text" name="nomorhp" class="textfield" id="nomorhp" value="" required/>  
                        <label>E-mail <em>*</em></label> 
                        <input type="email" name="email" class="textfield" id="email" value="" required/>
                        <label>Nama Bank <em>*</em></label>                           
                        <input type="text" name="bank" class="textfield" id="bank" value="" required/> 
                        <label>No.Rekening <em>*</em></label>                           
                        <input type="text" name="rekening" class="textfield" id="rekening" value="" required/>   
                        <label>Nama di Rekening <em>*</em></label>                           
                        <input type="text" name="namarek" class="textfield" id="namarek" value="" required/>   
                        <label>Password <em>*</em></label>                           
                        <input type="password" name="password" class="textfield" id="password" value="" required/> 
                        <label>Re-type Password <em>*</em></label>                           
                        <input type="password" name="password2" class="textfield" id="password2" value="" required/>
                        <label>PIN <em>*</em></label>                           
                        <input type="text" name="pin" class="textfield" id="pin" value="" required/> 
                        <label>Re-type PIN <em>*</em></label>                           
                        <input type="text" name="pin2" class="textfield" id="pin2" value="" required/> 
                        <div class="clear"></div> 
                        <label>&nbsp;</label>
                        <button type="submit" class="buttoncontact" id="proceed">Submit</button>
                        <span class="loading" style="display: none;">Please wait..</span>
                        <div class="clear"></div>
                        </fieldset> 
                    </form>
                    <!-- Contact Form End //-->
                </div>  
            </div>     
        </div>              
    </section>
<script src="/js/jquery.min.js"></script>
<script src="/scripts/register/1.js"></script>