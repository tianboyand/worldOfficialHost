<section class="content-wrapper">
        <div class="row">
            <div class="eleven column">
                <p class="lead">Transfer Ticket</p>
                <div id="contact-form-area">
                    <!-- Contact Form Start //-->
                    <% if(flash && flash.err) { %>
                      <ul class="alert alert-success" type="none">
                        <% Object.keys(flash.err).forEach(function(error) { %>
                          <li><%= JSON.stringify(flash.err[error]) %></li>
                        <% }) %>
                        </ul>
                    <% } %>
                    <form action="/user/sendticket" id="contactform"> 
                        <fieldset> 
                        <label>Ticket</label>                           
                        <input type="text" class="textfield" id="ticket" value="<%=session.User.ticket%>" readonly="true"/> 
                        <label>Username Tujuan <em>*</em></label>                           
                        <input type="text" name="username" class="textfield" id="username" value="" />
                        <label>Jumlah Ticket <em>*</em></label>                           
                        <input type="text" name="ticket" class="textfield" id="jlhtiket" value="" />
                        <div class="clear"></div> 
                        <label>&nbsp;</label>
                        <button type="submit" class="buttoncontact" id="buttonsend">Transfer</button>
                        <span class="loading" style="display: none;">Please wait..</span>
                        <div class="clear"></div>
                        </fieldset> 
                    </form>
                    <!-- Contact Form End //-->
                </div>  
            </div>     
        </div>              
    </section>